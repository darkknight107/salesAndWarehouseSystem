<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane fx:id="mainPanel" prefHeight="574.0" prefWidth="939.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="transferProduct.TransferController">
    <top>
        <GridPane hgap="10.0" vgap="10.0">
            <Label text="Sending Location: " GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <ComboBox fx:id="cbSendLocation" promptText="Choose Location" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="WRH1" />
                        <String fx:value="STR1" />
                        <String fx:value="STR2" />
                    </FXCollections>
                </items>
            </ComboBox>
            <Label text="Destination Location: " GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <ComboBox fx:id="cbDestinationLocation" promptText="Choose Location" GridPane.columnIndex="1" GridPane.rowIndex="2">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="WRH1" />
                        <String fx:value="STR1" />
                        <String fx:value="STR2" />
                    </FXCollections>
                </items>
            </ComboBox>
            <Label text="Description: " GridPane.columnIndex="0" GridPane.rowIndex="4" />
            <Button fx:id="btnNext" onAction="#handleNextButton" text="Next" GridPane.columnIndex="0" GridPane.rowIndex="5" />
         <TextArea fx:id="txtDescription" prefHeight="74.0" prefWidth="201.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </top>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</BorderPane>
